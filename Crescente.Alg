Algoritmo "Crescente"

Var

  x, y : inteiro


Inicio

   escreval("Digite dois numeros:")
   leia(x)
   leia(y)
   
   enquanto x <> y faca

      se x < y entao
         escreval("CRESCENTE")
      senao
         escreval("DECRECENTE")
      fimse
      
      escreval("Digite outros dois numeros:")
      leia(x)
      leia(y)
   
   
   fimenquanto

Fimalgoritmo
