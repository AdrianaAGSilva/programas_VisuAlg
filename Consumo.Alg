Algoritmo "Consumo"



Var

   distancia, consumo, combustivel : real

Inicio

   escreva("Distancia percorrida: ")
   leia(distancia)
   escreva("Combustivel gasto: ")
   leia(combustivel)

   consumo <-  distancia / combustivel
   
   escreval("Consumo medio de ", consumo:5:3)

Fimalgoritmo